{
	"name": "hello-inkor-irfnd",
	"version": "1.0.0",
	"type": "module",
	"scripts": {
		"prepare": "husky",
		"dev": "vite",
		"build": "tsc && vite build",
		"lint": "eslint \"src/**/*.{ts,tsx}\" --fix --report-unused-disable-directives --max-warnings 0",
		"format": "prettier --write \"**/*.{ts,tsx,cjs,js,json,md,html,css}\"",
		"preview": "vite preview"
	},
	"dependencies": {
		"axios": "^1.6.7",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"tailwind-scrollbar": "^3.1.0"
	},
	"devDependencies": {
		"@irfnd/prettier-config": "^1.0.0",
		"@types/node": "^20.11.26",
		"@types/react": "^18.2.64",
		"@types/react-dom": "^18.2.21",
		"@typescript-eslint/eslint-plugin": "^7.1.1",
		"@typescript-eslint/parser": "^7.1.1",
		"@vitejs/plugin-react-swc": "^3.5.0",
		"autoprefixer": "^10.4.18",
		"eslint": "^8.57.0",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-react-refresh": "^0.4.5",
		"husky": "^9.0.11",
		"lint-staged": "^15.2.2",
		"postcss": "^8.4.35",
		"prettier": "^3.2.5",
		"tailwindcss": "^3.4.1",
		"typescript": "^5.2.2",
		"vite": "^5.1.6"
	},
	"prettier": "@irfnd/prettier-config",
	"lint-staged": {
		"src/**/*.{ts,tsx}": "eslint --fix --report-unused-disable-directives --max-warnings 0",
